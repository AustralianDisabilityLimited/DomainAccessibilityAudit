{
  "name": "domain-accessibility-audit",
  "version": "0.1.0",
  "description": "Scan a domain and subdomains for accessibility violations and report them with a web interface.",
  "main": "index.js",
  "scripts": {
    "start:server": "cd backend && nodemon server.js --exec babel-node --presets es2015,stage-0",
    "start:client": "cd client && npm start",
    "start:dev": "concurrently \"npm run start:server\" \"npm run start:client\""
  },
  "author": "Damien Guillaume",
  "license": "GPL-3.0",
  "dependencies": {
    "axe-webdriverjs": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "crypto": "^1.0.1",
    "express": "^4.17.0",
    "express-session": "^1.16.2",
    "mongoose": "5.5.10",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.0",
    "selenium-webdriver": "^3.6.0",
    "xml2js": "^0.4.19"
  }
}
